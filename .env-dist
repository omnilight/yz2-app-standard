# This file contains development defaults for ENV variables

# Framework
# ---------

YII_DEBUG=true
YII_ENV=dev
YII_TRACE_LEVEL=0

# Application
# -----------

APP_ID=yz2-app-standard
APP_NAME=My Application
APP_FRONTEND_COOKIE_VALIDATION_KEY=
APP_BACKEND_COOKIE_VALIDATION_KEY=

# Database
# --------

DB_PORT_3306_TCP_ADDR=localhost
DB_PORT_3306_TCP_PORT=3306
DB_ENV_MYSQL_DATABASE=yz2app
DB_ENV_MYSQL_USER=yz2app
DB_ENV_MYSQL_PASSWORD=yz2app

DATABASE_DSN=mysql:host={$DB_PORT_3306_TCP_ADDR};port={$DB_PORT_3306_TCP_PORT};dbname={$DB_ENV_MYSQL_DATABASE}
DATABASE_USER={$DB_ENV_MYSQL_USER}
DATABASE_PASSWORD={$DB_ENV_MYSQL_PASSWORD}
DATABASE_TABLE_PREFIX=yz_