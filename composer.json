{
    "name": "omnilight/yz2-app-standard",
    "description": "Standard application based on Yii2 and Yz2",
    "keywords": ["yii", "framework", "yz", "application template"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pavel Agalecky",
            "email": "pavel.agalecky@gmail.com"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "http://git.zakazpodarka.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "omnilight/yz2": "*",
        "omnilight/yz2-admin": "*",
        "omnilight/yz2-gii": "*"
    },
    "autoload": {
        "psr-4": {
            "frontend\\": "app/frontend",
            "backend\\": "app/backend",
            "console\\": "app/console",
            "modules\\": "app/modules",
            "yz2app\\carcass\\": "carcass"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission",
            "yz2app\\carcass\\Configurator::postCreateProject"
        ]
    },
    "config": {
        "vendor-dir": "app/vendor",
        "bin-dir": "app/vendor/bin"
    },
    "bin": ["app/vendor/bin"],
    "extra": {
        "writable": [
            "app/frontend/runtime",
            "app/backend/runtime",
            "app/console/runtime"
        ],
        "executable": [
            "app/bin/yii"
        ]
    }
}